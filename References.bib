@INPROCEEDINGS{Zuluaga2011b,
  author = {Zuluaga, M. A. and Delgado Leyton, E. J. F. and Hern{\'a}ndez Hoyos,
	M. and Orkisz, M.},
  title = {Feature Selection for SVM-Based Vascular Anomaly Detection},
  booktitle = {Medical Computer Vision. Recognition Techniques and Applications
